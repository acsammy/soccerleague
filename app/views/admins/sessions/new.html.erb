<div class="limiter">
  <div class="container-login100">
    <div class="wrap-login100 p-t-50 p-b-90">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name), class:"login100-form validate-form flex-sb flex-w") do |f| %>
        <span class="login100-form-title p-b-51">
          Login
        </span>

        <div class="wrap-input100 validate-input m-b-16">
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "input100", placeholder: "Email" %>
          <span class="focus-input100"></span>
        </div>


        <div class="wrap-input100 validate-input m-b-16" data-validate = "Password is required">
          <%= f.password_field :password, autocomplete: "current-password", class: "input100", placeholder: "Senha" %>
          <span class="focus-input100"></span>
        </div>

        <div class="flex-sb-m w-full p-t-3 p-b-24">
          <div class="contact100-form-checkbox">
            <% if devise_mapping.rememberable? -%>
              <div class="field">
                <%= f.check_box :remember_me, class:"input-checkbox100" %>
                <%= f.label :remember_me, class:"label-checkbox100" %>
              </div>
            <% end -%>
          </div>

          <div>
            <a href="#" class="txt1">
            <%= render "devise/shared/links" %>
            </a>
          </div>
        </div>

        <div class="container-login100-form-btn m-t-17">
          <button class="login100-form-btn btn btn-success">
            Login
          </button>
        </div>

      <% end %>
    </div>
  </div>
</div>


<div id="dropDownSelect1"></div>
